{
  "name": "merge-nb",
  "displayName": "MergeNB",
  "description": "Rich Jupyter Notebook Merge Conflict Resolver",
  "version": "0.0.3",
  "repository": "https://github.com/Avni2000/MergeNB",
  "engines": {
    "vscode": "^1.108.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:jupyter-notebook",
    "workspaceContains:**/*.ipynb"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "merge-nb.findConflicts",
        "title": "Find Notebooks with Merge Conflicts",
        "category": "MergeNB",
        "icon": "$(git-merge)"
      },
      {
        "command": "merge-nb.resolveCurrentFile",
        "title": "Resolve Merge Conflicts",
        "category": "MergeNB",
        "icon": "$(warning)"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "when": "resourceExtname == .ipynb",
          "command": "merge-nb.findConflicts",
          "group": "3_compare"
        }
      ],
      "editor/title": [
        {
          "when": "resourceExtname == .ipynb && mergeNB.hasConflicts",
          "command": "merge-nb.resolveCurrentFile",
          "group": "navigation"
        }
      ],
      "commandPalette": [
        {
          "command": "merge-nb.findConflicts",
          "when": "true"
        },
        {
          "command": "merge-nb.resolveCurrentFile",
          "when": "resourceExtname == .ipynb && mergeNB.hasConflicts"
        }
      ]
    },
    "keybindings": [
      {
        "command": "merge-nb.findConflicts",
        "key": "ctrl+shift+m",
        "mac": "cmd+shift+m",
        "when": "resourceExtname == .ipynb"
      }
    ],
    "configuration": {
      "title": "MergeNB",
      "properties": {
        "mergeNB.ui.hideNonConflictOutputs": {
          "type": "boolean",
          "default": false,
          "description": "Hide outputs from cells without conflicts in the conflict resolution view"
        },
        "mergeNB.autoResolve.executionCount": {
          "type": "boolean",
          "default": true,
          "description": "Automatically resolve execution count differences by setting to null"
        },
        "mergeNB.autoResolve.kernelVersion": {
          "type": "boolean",
          "default": true,
          "description": "Automatically resolve kernel version differences by using current version"
        },
        "mergeNB.autoResolve.stripOutputs": {
          "type": "boolean",
          "default": true,
          "description": "Automatically strip outputs from cells with conflicts"
        },
        "mergeNB.ui.showCellHeaders": {
          "type": "boolean",
          "default": false,
          "description": "Show cell type, execution count, and cell index in the conflict resolution view"
        },
        "mergeNB.ui.enableUndoRedoHotkeys": {
          "type": "boolean",
          "default": true,
          "description": "Enable Ctrl+Z / Ctrl+Shift+Z undo and redo shortcuts in the conflict resolution web UI"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test",
    "pretest:integration": "npm run compile && npm run compile-tests",
    "pretest:integration:all": "npm run compile && npm run compile-tests",
    "test:integration": "node out/tests/runIntegrationTest.js",
    "test:integration:all": "node out/tests/runIntegrationTest.js --all",
    "test:integration:list": "npm run compile-tests && node out/tests/runIntegrationTest.js --list"
  },
  "devDependencies": {
    "@clack/prompts": "^1.0.0",
    "@playwright/test": "^1.58.1",
    "@types/dompurify": "^3.0.5",
    "@types/markdown-it": "^14.1.2",
    "@types/mocha": "^10.0.10",
    "@types/node": "25.x",
    "@types/react": "^18.3.27",
    "@types/react-dom": "^18.3.7",
    "@types/vscode": "^1.108.1",
    "@types/ws": "^8.18.1",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2",
    "esbuild": "^0.27.2",
    "eslint": "^9.39.2",
    "mocha": "^11.7.5",
    "module-alias": "^2.3.3",
    "npm-run-all": "^4.1.5",
    "picocolors": "^1.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.1"
  },
  "dependencies": {
    "@vscode/markdown-it-katex": "^1.1.2",
    "dompurify": "^3.3.1",
    "katex": "^0.16.22",
    "markdown-it": "^14.1.0",
    "ws": "^8.19.0"
  }
}
