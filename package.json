{
  "name": "merge-nb",
  "displayName": "MergeNB",
  "description": "Rich Jupyter Notebook Merge Conflict Resolver",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.107.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:jupyter-notebook",
    "workspaceContains:**/*.ipynb"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "merge-nb.findConflicts",
        "title": "Find Notebooks with Merge Conflicts",
        "category": "MergeNB",
        "icon": "$(git-merge)"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "when": "resourceExtname == .ipynb",
          "command": "merge-nb.findConflicts",
          "group": "3_compare"
        }
      ],
      "editor/title": [
        {
          "when": "resourceExtname == .ipynb",
          "command": "merge-nb.findConflicts",
          "group": "navigation"
        }
      ],
      "commandPalette": [
        {
          "command": "merge-nb.findConflicts",
          "when": "true"
        }
      ]
    },
    "keybindings": [
      {
        "command": "merge-nb.findConflicts",
        "key": "ctrl+shift+m",
        "mac": "cmd+shift+m",
        "when": "resourceExtname == .ipynb"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.107.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "typescript-eslint": "^8.46.3",
    "eslint": "^9.39.1",
    "esbuild": "^0.27.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.9.3",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2"
  }
}
